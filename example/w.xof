%% User Data for dsdxof octave script


% FS File Settings

% Input file
FSInputFile="w.frd";
% Output directory
FSOutDir="./";


% GS General Settings

% Sampling frequency of final filters
% May be omitted if given as command line parameter
GSFs=48000;


% CF Crossover Filter

% Crossover class: 'mp', minimum phase; 'lp', linear phase
% May be omitted if given as command line parameter
CFClass = 'lp';

% Crossover type: Must be a cell array
CFLowType={"B"};
% Low feq. crossover order (for minimum phase varieties)
CFLowOrder=[2];
% Crossover point frequency Low (Hz)
CFLowF=[45];
% If CFClass = 'lp' and CFLowType="Butterworth"
% mimics the magnitude of a Butterwoth high pass
CFLowAsMP=true;
                            
% Crossover type
CFHighType="LR";
% High feq. crossover order (for minimum phase varieties)
CFHighOrder=4;
% Crossover point frequency High (Hz)
CFHighF=200;

%% End user data
